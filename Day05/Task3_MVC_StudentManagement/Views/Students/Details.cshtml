@model StudentManagement.Models.Student
@{ ViewData["Title"] = "Details"; }
<h2>Details</h2>
<dl class="row"><dt class="col-sm-2">ID</dt><dd class="col-sm-10">@Model.Id</dd><dt class="col-sm-2">Name</dt><dd class="col-sm-10">@Model.Name</dd><dt class="col-sm-2">BirthDate</dt><dd class="col-sm-10">@Model.BirthDate.ToShortDateString()</dd><dt class="col-sm-2">Address</dt><dd class="col-sm-10">@Model.Address</dd><dt class="col-sm-2">School</dt><dd class="col-sm-10">@Model.SchoolName</dd><dt class="col-sm-2">Percentage</dt><dd class="col-sm-10">@Model.Percentage</dd></dl><a class="btn btn-secondary" href="/Students">Back to list</a>
