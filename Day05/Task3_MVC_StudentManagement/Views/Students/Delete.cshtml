@model StudentManagement.Models.Student
@{ ViewData["Title"] = "Delete"; }
<h2>Delete</h2><h3>Are you sure?</h3>
<dl class="row"><dt class="col-sm-2">Name</dt><dd class="col-sm-10">@Model.Name</dd><dt class="col-sm-2">School</dt><dd class="col-sm-10">@Model.SchoolName</dd></dl>
<form asp-action="Delete" method="post"><input type="hidden" asp-for="Id" /><button type="submit" class="btn btn-danger">Delete</button><a class="btn btn-secondary" href="/Students">Cancel</a></form>
